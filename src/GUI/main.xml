R"zk1sakz(
<root>
    <styles>
        <include src="file://{resources}/styles/csgostyles.css" />
        <include src="file://{resources}/styles/settings/settings.css" />
        <include src="file://{resources}/styles/debugger.css" />
    </styles>
    <Panel class='Main' style='width: 700px; height: 500px; horizontal-align: center; vertical-align: center; background-color: black;'>
      <CSGOBlurTarget id="SkeleMainMenuCore" blurrects="ChatContainer mainmenu-sidebar__blur-target CSGOLoadingScreen JsMainMenuNavBar eom-winner">

        <Label text='Skeletux' style='horizontal-align: center; vertical-align: top; font-size: 24px; color: red; text-decoration: underline;'/>
        <Panel style='vertical-align: middle; horizontal-align: left; flow-children: down;'>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <Button style='width: 300px; height: 80px;' onactivate='$.Msg("1st Button Pressed");$.DispatchEvent( "PlaySoundEffect", "mainmenu_press_GO", "MOUSE" );'>
              <Label text='Babbys 1st Btn' style='background-color: grey;'/>
            </Button>
          </Panel>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <Button onactivate='$.Msg("2nd Button Pressed");$.DispatchEvent( "PlaySoundEffect", "mainmenu_press_GO", "MOUSE" );'>
              <Label text='Babbys 2nd Btn' style='background-color: black;'/>
            </Button>
          </Panel>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <Label text="Enable ESP"/>
          </Panel>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <CSGOSettingsEnumDropDown class="PopupButton White" convar="skele_enable_esp">
              <Label text="#GameUI_No" value="0" id ="ppOff"/>
              <Label text="#GameUI_Yes" value="1" id ="ppOn"/>
            </CSGOSettingsEnumDropDown>
          </Panel>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <Label text="No Post Processing"/>
          </Panel>
          <Panel style='horizontal-align: left; margin-bottom: 20px;'>
            <CSGOSettingsEnumDropDown class="PopupButton White" convar="skele_disable_pp">
              <Label text="#GameUI_No" value="0" id ="fogOff"/>
              <Label text="#GameUI_Yes" value="1" id ="fogOn"/>
            </CSGOSettingsEnumDropDown>
          </Panel>
        </Panel>
      </CSGOBlurTarget>
    </Panel>
</root>

)zk1sakz"